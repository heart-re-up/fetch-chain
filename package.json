{
	"name": "fetch-chain",
	"version": "0.0.3",
	"description": "A fetch client with interceptors",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"dev": "ts-node src/index.ts",
		"build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts --clean",
		"prepare": "pnpm build",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write \"src/**/*.ts\""
	},
	"keywords": [
		"fetch",
		"interceptor",
		"http",
		"typescript"
	],
	"author": "heart-re-up",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heart-re-up/fetch-chain.git"
	},
	"homepage": "https://github.com/heart-re-up/fetch-chain#readme",
	"bugs": {
		"url": "https://github.com/heart-re-up/fetch-chain/issues"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.22.0",
		"eslint": "^9.19.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	}
}